{
  "name": "nrel/nrel_bootstrap_module",
  "description": "Companion module to the NREL Bootstrap theme.",
  "type": "drupal-module",
  "license": "GPL-2.0",
  "prefer-stable": true,
  "minimum-stability": "dev",
  "homepage": "https://github.com/NREL/nrel_bootstrap_module",
  "authors": [
    {
      "name": "Shaun Laws (slaws)",
      "email": "shaun.laws@nrel.gov",
      "role": "Co-maintainer"
    },
    {
      "name": "Chris Modern (cmodern)",
      "email": "christian.modern@nrel.gov",
      "role": "Co-maintainer"
    }
  ],
  "support": {
    "issues": "https://github.com/NREL/nrel_bootstrap_module/issues"
  },
  "repositories": {
    "drupal": {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    },
    "assets": {
      "type": "composer",
      "url": "https://asset-packagist.nrel.gov/nrel/drupal8/",
      "options": {
          "ssl": {
              "verify_peer": false,
              "allow_self_signed": true
          }
      }
  }
},
  "require": {
    "nrel/nrel_bootstrap": "dev-bootstrap5",
    "drupal/core": "^10.3"
  }
}
